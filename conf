projdir: cmpr/
revdir: cmpr/revs
tmpdir: cmpr/tmp
builddir: cmpr/dist
projfile: cmpr/cmpr.c
buildcmd: (D=$(date +%Y%m%d-%H%M%S); gcc -o cmpr/dist/cmpr-$D -fsanitize=address cmpr/cmpr.c -g -lm && rm -f cmpr/dist/cmpr && ln -s cmpr-$D cmpr/dist/cmpr)
cbcopy: xclip -i -selection clipboard
cbpaste: xclip -o -selection clipboard
blockdef: C
